  "dependencies": {
    "@better-scroll/core": "^2.4.2",
    "@better-scroll/mouse-wheel": "^2.4.2",
    "@better-scroll/pull-up": "^2.4.2",
    "@vant/area-data": "^1.2.4",
    "animate.css": "^4.1.1",
    "axios": "^0.26.1",
    "vant": "^3.4.9",
    "vue": "^3.2.25",
    "vue-axios": "^3.4.1",
    "vue-jsonp": "^2.0.0",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@vicons/fa": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@yuo/postcss-px2vw": "^1.0.5",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^13.0.0",
    "naive-ui": "^2.28.3",
    "prettier": "2.6.2",
    "sass": "^1.49.11",
    "vfonts": "^0.0.3",
    "vite": "^2.9.5",
    "vite-plugin-style-import": "^1.4.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix"
    ]
  }
